.side-bar__my-account
  %p.side-bar__my-account__name
    = current_user.name
  .p.side-bar__my-account__icons
    = link_to "#", method: :get, class: "side-bar__my-account__create" do
      = fa_icon("edit")
    = link_to edit_user_path(current_user), method: :get, class: "side-bar__my-account__edit" do
      = fa_icon("cog")
